    "build:css": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js ./styles -o ./app/styles",
    "dev:css": "npm run build:css && node-sass --importer node_modules/node-sass-package-importer/dist/cli.js -rw ./styles -o ./app/styles",
